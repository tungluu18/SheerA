{
  "presets": [
    "@babel/preset-env"
  ],
  "ignore": [
    "node_modules"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "app": "./src/app",
          "socket": "./src/socket",
          "index": "./src/index",
          "routes": "./src/routes",
          "socket-hooks": "./src/socket-hooks"
        }
      }
    ]
  ]
}
